{
  "name": "@cantrips/core",
  "version": "0.13.20",
  "description": "Cantrips-core for more descriptive builds",
  "repository": "https://github.com/buildcantrips/core.git",
  "main": "./lib/index.js",
  "author": "csaba.makara@gmail.com",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 30,
    "branches": 40,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/index.js"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "cache": false,
    "all": true
  },
  "dependencies": {
    "@babel/runtime": "7.1.2",
    "ora": "^3.0.0",
    "requirejs": "^2.3.6",
    "simple-git": "^1.106.0",
    "winston": "^3.1.0"
  },
  "scripts": {
    "test": "mocha --compilers js:@babel/register --timeout 10000 --recursive src",
    "test:ci": "nyc mocha --compilers js:@babel/register --timeout 20000 --recursive src",
    "lint": "eslint src",
    "babel:build": "babel src -d lib --ignore node_modules,src/modules",
    "babel:watch": "babel src -d lib -w --ignore node_modules,src/modules"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^23.6.0",
    "jest-spec-reporter": "^1.0.5",
    "mocha": "^5.2.0",
    "nyc": "^11.4.1",
    "tmp": "0.0.33"
  }
}
